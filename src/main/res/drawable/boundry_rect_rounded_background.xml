<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle" >
            <!--<padding android:top="2dip" android:left="2dip" android:bottom="2dip" android:right="2dip"/>-->

            <corners android:radius="10dp"></corners>
            <padding android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"></padding>
            <stroke
                android:width="2dp"
                android:color="#ff8300" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp"></corners>
            <solid
                android:color="#44ffffff" />
        </shape>
    </item>
    <!--android:color="#FFD700" />-->


</layer-list>